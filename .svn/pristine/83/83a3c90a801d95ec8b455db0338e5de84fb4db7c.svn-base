/* banner */

.carousel {

	overflow: hidden;

	border-radius: .8rem;

	overflow: hidden;

}

.carousel-indicators li {

	padding: 0;

	width: 20px;

    height: 20px;

	cursor: pointer;

	border-radius: 50%;

}

.carousel-indicators li.active {

	background: #85398b;

}

.carousel-indicators li::before ,

.carousel-indicators li::after {

	display: none;

}

.carousel-item { 

	background: #000;

	height: 500px;

}

.carousel-item img {

	 width: 100%;

	display: block;	 

 } 

.carousel.fade {

	opacity: 1;

}

.carousel.fade .carousel-item {

   transition: opacity ease-out .7s;

   left: 0;

   opacity: 0; /* hide all slides */

   top: 0;

   position: absolute;

   width: 100%;

   display: block;

}

.carousel.fade .carousel-item:first-child {

   top: auto;

   opacity: 1; /* show first slide */

   position: relative;

}

.carousel.fade .carousel-item.active {

   opacity: 1;

}

/* banner end */

/* login container */

.login-container2 {

	background: #85398b;

	padding: 2rem 3rem;

	border-radius: 1.5rem;

	color: #fff;

} 

.login-container2 h3 {

	position: relative;

	margin-bottom: 3.5rem;

}

.login-container2 h3::before {

	content: '';

