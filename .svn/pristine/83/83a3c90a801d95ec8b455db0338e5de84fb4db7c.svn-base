/* news */ 

.news-lists {  

  	position: relative;

	overflow-y: auto;

	height: 390px;

	border: 0 !important;

	flex-wrap: nowrap;

	padding-right: 1rem;

} 

.news-lists::-webkit-scrollbar, 
.social-feed::-webkit-scrollbar,
.gallery-carousel::-webkit-scrollbar, 
.partner-card::-webkit-scrollbar,
.testimonial-card .card-body::-webkit-scrollbar {

	width: 5px;

	height: 5px;

	background-color: #ddd;

	border-radius: .25rem;

}

.gallery-carousel::-webkit-scrollbar-thumb,
.partner-card::-webkit-scrollbar-thumb ,
.news-lists::-webkit-scrollbar-thumb,
.social-feed::-webkit-scrollbar-thumb,
.testimonial-card  .card-body::-webkit-scrollbar-thumb {

	background-color: #85398b;

	border-radius: .25rem;

}

.news-lists a { 

	margin-bottom: .5rem;

	position: relative; 

	display: flex; 

	padding: .8rem;

	cursor: pointer;

	border-radius: 1rem !important;

	border: none !important; 

	color: inherit;

}
.hovereffect .overlay {
    padding: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
}
.news-lists a img { 
	border-radius: .5rem;  
    width: 70px;
    height: 56px; 
    margin-right: 1rem;
    object-fit: cover;
    margin-top: 1rem;

}

.news-lists a .news-date { 

	opacity: .6;

}

.news-lists a:hover,.news-lists a.active {

	background: #f7eaf8 !important;

}

.news-lists a p { 

	margin: 0 0 0.5rem;

	font-size: 1.125rem;

	text-align: left;

} 

.news-preview { 

	 position: relative; 

	 overflow: hidden;

	 height: 580px;

}

.tab-pane {

height: 100%;

}

.news-preview ,.news-preview img,.news-details{ 

	height: 100%;   

}

.news-preview img {

	display: block; 

	width: 100%;

	border-radius: .5rem;

	object-fit: cover;

	object-position: top;

}  

 

.news-details {

	border-radius: .5rem;

    color: #fff;

    background: linear-gradient(to top, black, transparent);

    position: absolute;

    top: 0;

    left: 0;

    width: 100%;

    height: 100%;

    display: flex;

    flex-direction: column;

    justify-content: flex-end;

    padding: 2rem; 

}

.news-details h2 {

	font-weight: 500;

	font-size: 2rem;

 }

