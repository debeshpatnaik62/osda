/* testimonial */
.card-shadow {
	overflow: hidden;
	border-radius: 12px;
	box-shadow: 0px 4px 13px #ddd;
	transition: .3s;
	border: 1px solid transparent; 
	height: 100%;
} 
.testimonial-card .card-body {
    overflow-y: auto;
}
.testimonial-card-img {
	width: 80px;
	height: 80px; 
	position: relative;
}
.testimonial-card-img img {
	width: 100%;
	object-position: center;
	object-fit: cover;
	border-radius: 50%;
}
.testimonial-card-img  .fa-quote-right { 
    background: #963a88;
    width: 20px;
    height: 20px;
    position: absolute;
    bottom: 5px;
    right: 6px;
    z-index: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
    font-size: 0.6rem;
	color: #fff;
} 
.testimonial-card-name {
	color: #b345b0;
}
.testimonial-card-occupation {
	color: #adadad; 
}
.bg-quote {  
    position: absolute;
	bottom: -50px;
    right: -100px;
}
.bg-quote svg {
	fill: rgb(149 57 136 / 8%); 
	width: 100%;
	height: 100%;
}
/* testimonial end */
/* gallery */
 
.gallery-carousel .galleryNew  img {
	object-fit: cover;
	object-position: center;
	max-height: 100%;
	transform: none;
}
.gallery-carousel .galleryNew {
	 border-radius: 12px; overflow: hidden;
	height: 100%;
 }
 .galleryNew p, .galleryNew:hover .multi_hover {
    display: block;
    top: initial;
    left: initial;
    transform: none;
    padding: 0;
    position: static;
    cursor: pointer;
}
.gallery-carousel .galleryinner {
    display: flex; 
    justify-content: space-between;
    flex-wrap: wrap;
}
.gallery-carousel .galleryinner li {
    flex: 0 0 48%;
    margin-bottom: 1rem;
}
.gallery-carousel, .partner-card {
    height: calc(100% - 80px);
    overflow: auto;
}
/* gallery end */
/* partner-card */
.partner-card ul {
	display: flex;
	flex-wrap: wrap;
	margin-bottom: 0;
}
.partner-card ul li {
	flex: 0 0 50%;
	display: flex;
	align-items: center;
	justify-content: center; 
	padding: 1.5rem;
	margin-bottom: 0;
    border-bottom: 1px solid #e8e8e8;
} 
.partner-card ul li:last-child,.partner-card ul li:nth-last-child(2) {
    border-bottom: 0;
}
.partner-card ul li img {
	max-width: 100%;
	transition: .3s;
}
.partner-card ul li img:hover {
	filter: grayscale(1);
}
/* partner-card end */
/* social */
.social-feed {
	height: 320px;
    overflow: auto;
    margin: 2.5rem 0 0;
    border-radius: 12px;
    box-shadow: 0px 4px 13px #ddd;
}
