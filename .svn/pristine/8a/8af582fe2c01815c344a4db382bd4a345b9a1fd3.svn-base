/* login container end */
.partner-info .owl-stage-outer{
	padding: 1rem .25rem;
}
.partner-info .owl-stage-outer .owl-item { 
    box-shadow: 0px 3px 10px #ddd; 
    border-radius: 8px; 
	cursor: pointer;
	transition: .3s;
	border: 1px solid transparent;
}
.partner-info .owl-stage-outer .owl-item:hover {
	box-shadow: none;
	border: 1px solid #ddd;
}
.partner-info .owl-stage-outer .owl-item a {
	padding: .5rem;
	height: 100px;
}
.partner-info .owl-stage-outer .owl-item img {
	max-height: 100%;
    object-fit: contain;
}
.partner-info h2{
	position: relative; 
	margin: 0 0 1rem;
}
.partner-info h2:before {
	content: '';
	position: absolute;
	left: 0; 
	bottom: -8px;
	width: 100px;
	height: 2px;
	background: linear-gradient(45deg, #663891 0, #ffffff00 100%);
}
.partner-info  .owl-nav {
	position: absolute;
	top: -2.5rem;
	right: 0;
} 
.partner-info  .owl-nav button.owl-prev  {
	margin-right: 1rem;
} 
.partner-info  .owl-nav button.owl-prev ,
.partner-info  .owl-nav button.owl-next {
	outline: none !important;
    border: none !important;
}
.partner-info  .owl-nav button.owl-prev span,
.partner-info  .owl-nav button.owl-next span
{ 
	width: 30px;
    height: 30px;
    font-size: 2rem;
    line-height: 80%;
    background: #eaeaea;
    display: inline-block;
    color: #6d6d6d;
    border-radius: 3px;
}
.partner-info  .owl-nav button.owl-prev span:hover ,
.partner-info  .owl-nav button.owl-next span:hover {
	background: #663891;
	color: #fff;
}


/* news */ 
.news-lists {  
  	position: relative;
	overflow-y: auto;
	height: 580px;
	border: 0 !important;
	flex-wrap: nowrap;
	padding-right: 1rem;
} 
.news-lists::-webkit-scrollbar {
	width: 5px;
	height: 5px;
	background-color: #f1f1f1;
	border-radius: .25rem;
}
.news-lists::-webkit-scrollbar-thumb {
	background-color: #85398b;
	border-radius: .25rem;
}
.news-lists a { 
	margin-bottom: .5rem;
	position: relative; 
	display: flex; 
	padding: .8rem;
	cursor: pointer;
	border-radius: 1rem !important;
	border: none !important; 
	color: inherit;
}
.news-lists a img {
	border-radius: .5rem;
	width: 150px;
	height: 100px;
	margin-right: 1rem;
	object-fit: cover;
}
.news-lists a .news-date { 
	opacity: .6;
}
.news-lists a:hover,.news-lists a.active {
	background: #f7eaf8 !important;
}
.news-lists a p { 
	margin: 0 0 0.5rem;
	font-size: 1.125rem;
	text-align: left;
} 
.news-preview { 
	 position: relative; 
	 overflow: hidden; 
}
.tab-pane {
	height: 580px;
}
.news-preview ,.news-preview img,.news-details{ 
	height: 100%;   
}
.news-preview img {
	display: block; 
	width: 100%;
	border-radius: .5rem;
	object-fit: cover;
	object-position: top;
}  
 
.news-details {
	border-radius: .5rem;
    color: #fff;
    background: linear-gradient(to top, black, transparent);
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    padding: 2rem; 
}
.news-details h2 {
	font-weight: 500;
	font-size: 2rem;
 }
/* news edn */
@media   (min-width:1200px)  and (max-width:1500px) {
