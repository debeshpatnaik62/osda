body{
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px; padding:0px; margin:0px 20px;;
	}
.clear{clear:both; height:1px; font-size:0px; line-height:0px;}
#header{
	font-size:18px;
	font-weight:bold;
	font-family:Arial, Helvetica, sans-serif;
	color:#333;
	padding-bottom: 5px;
	border-bottom: #000000 solid 1px;
	}
.text_logo{margin:10px 0px 8px 0px; padding:0px;}
.text_logo h1 {color: #000; margin-top: 8px;}
.text_logo h1.logo{font-size:26px; line-height:18px; padding:0px; margin:0px; text-align:left; padding-left:10px; padding-top:16px; color:#000;}
.text_logo h1.logo small{font-size:13px; color:#000;}
.text_logo h1.logo img{float:left; margin-right:15px; margin-top:-2px;}
.text_logo h1.logo span{font-size:13px;}
#header img.logo, pull-left{float:left;}
#header img.logo{margin-right:10px;}
#printHeader{
	text-align:left;
	font-size:16px;
	font-weight:bold;
	padding-top:10px;
	padding-bottom:10px;
	}

	.text-success {
    color: #28a745!important;
}

.noPrint{
	display:none;
}
table{
	border:#CCCCCC solid 1px;
	border-collapse:collapse;
	width: 100%;
	margin-bottom: 10px;	
}
table th{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #000000;
	text-decoration: none;
	background-color:#E8E8E8;
	border:#CCCCCC solid 1px;
	border-collapse:collapse;
	text-align: left;
	padding: 2px 5px;
}
table td{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: normal;
	color: #000000;
	text-decoration: none;
	border:#CCCCCC solid 1px;
	border-collapse:collapse;
	text-align: left;
	padding: 2px 5px;
}
#header h1{font-size:18px; color:#C5322B;text-align:left;  margin: 0px;padding: 0px;}
#printFooter{    border-top: #000000 solid 1px;
    color: #000;
    padding-top: 13px;
    margin-top: 20px;
    
    
    
    text-align: center;
    }

.checklistTable table{border:0px;}
.checklistTable table th{border:0px;}
.checklistTable table td{vertical-align:middle; border:0px; border-bottom:#ccc solid 1px;}
.checklistTable table td .bottomBorder{border-bottom:#000 solid 1px; width:150px; height:24px; display: inline-block;}
.checklistTable table td .chkBox{border:#000000 solid 1px; width:24px; height:24px; margin:0px 15px -8px 15px; display: inline-block;}
.checklistTable table td .remarkBox{border:#000000 solid 1px; width:250px; height:60px; margin:5px 0px; display: inline-block;}

