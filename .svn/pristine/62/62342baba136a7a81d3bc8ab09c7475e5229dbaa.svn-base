svg#English{height:460px;}.schemes-btn{cursor: pointer;}.pyramid{border-bottom:1px solid #e4e3e3;padding:0 0 2em 0;margin:0 0 2em 0}.schemes{border:1px solid #e4e4e4;background-color:#fff;margin:0 0 1em 0;padding:1em;border-radius:2px}.schemes__media{background:linear-gradient(#c3c3c3,#e4e4e4,#ccc);height:76px;padding:1em}.schemes__media img{margin:auto;display:block}.schemes__title{margin-bottom:0}.schemes__title a{color:#000}.schemes__body{border-top:1px solid #ccc;padding:.5em 0 0}.scheme-pyramid{display:none}.schemes__heading .text-secondary{text-align:left}@media screen and (min-width:380px){.schemes__media{width:40%;display:inline-block;vertical-align:top}.schemes__heading{padding:0 .3em;display:inline-block;width:58%}}@media screen and (min-width:430px){.schemes__media{height: 115px;}.schemes__heading{padding:0 .3em 0 .5em}}@media screen and (min-width:430px){.schemes__media{display:inline-grid;align-items:center}}@media screen and (min-width:630px){.scheme-pyramid-mob{display:none}.scheme-pyramid{display:block}}@media screen and (min-width:992px){.schemes__media{height:103px}}@media screen and (min-width:992px){.schemes__media{height:115px}}@media screen and (max-width:430px){.schemes__media{height:113px}}